package net.minecraft.level.entities;

import net.minecraft.level.Level;

public class Player extends Entity
{
    public String name;
    
    public Player(Level level, String name)
    {
        super(level);
        this.name = name;
    }
    
    public void tick()
    {
        super.tick();
        float forward = 0, strafe = 0;
        
        
    }
}
