import net.minecraft.*;
import net.minecraft.utils.Ticker;

public class BuilderThread
{
    public static void start()
    {
        Thread buildy = new Thread(() ->
        {
            while (Window.isOpen())
            {
                
            }
        });
        
        buildy.setName("Buildy Thread");
        buildy.start();
    }
}
